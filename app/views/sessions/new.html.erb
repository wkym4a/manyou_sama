<!--↓タイトル表示枠-->
<% content_for :show_head_title do%>
  <h1 class="title_head">ログイン画面</h1>
<% end %>
<!--↑タイトル表示枠-->

<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

  <%= f.label :cd %>
  <%= f.email_field :cd %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Log in" %>
<% end %>
